<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="t.o.j.mapper.userM">
  
   <select id="selectByid" resultType="userVO">
 		select * from tk_account where id = #{id}
     </select>
 	
     <insert id="insert"> 
   		insert into tk_account (id, passwd, username, mobile, email) values
 		(#{id}, #{passwd}, #{username}, #{mobile}, #{email})
     </insert> 

</mapper>  